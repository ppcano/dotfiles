[user]
  name = Pepe Cano
  email = ppcanodehuelva@gmail.com

[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  g = grep -I
  lg = log -p
  pb = publish-branch
  pp = !"echo 'Pull'; git pull; echo 'Push'; git push"
  rb = rbranch
  rc = rank-contributors
  rv = review
  sm = show-merges
  st = status
	ac = !git add -A && git commit -am
  lg = !git log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[apply]
whitespace = fix
[core]
excludesfile = ~/.gitignore
attributesfile = ~/.gitattributes
whitespace = fix,space-before-tab,tab-in-indent,trailing-space
config = /usr/bin/vim
editor = /usr/bin/vim
[color]
ui = auto
[color "branch"]
current = yellow reverse
local = yellow
remote = green
[color "diff"]
meta = yellow bold
frag = magenta bold
old = red bold
new = green bold
[color "status"]
added = yellow
changed = green
untracked = cyan
[merge]
log = true
[push]
default = current
